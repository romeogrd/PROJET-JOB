<%- include('./partials/header') -%>

<h1>Welcome to the homepage</h1>
<h1><a href="/myprofile">My Profile</a></h1>


<% if (user) {
    user.jobs.forEach(job => { %>
        <div class="job">
            <p><%= job._id %></p>
            <p><%= job.jobtitle %></p>
            <p><%= job.nameEmployer %></p>
            <p><a href="/jobdetail">See more</a></p>
        </div>
<% }) } %>





<a href="/newjob">Create a new job</a>

 

